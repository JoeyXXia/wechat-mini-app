<!--components/menu-area/menu-area.wxml-->
<view class="menu" wx:if="{{menuList.length}}">
  <area-header title="{{ title}}" />
<scroll-view 
class="list"
scroll-x
enable-flex
style="width: {{screentWidth}}px;"
>
<block wx:for="{{menuList}}" wx:key="id">
<view class="id">
<menu-item/>
</view>

</block>

</scroll-view>
</view>
